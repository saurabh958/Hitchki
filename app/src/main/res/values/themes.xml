        <!-- Customize your light theme here. -->
         <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorSecondary">@color/colorPrimaryLight</item>
    </style>

    <style name="Theme.Hitchki" parent="Base.Theme.Hitchki" />

    <style
